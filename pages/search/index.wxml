<view class="search_row">
    <input placeholder="SearcH YoU WanT" bindinput="handleInput"></input>
</view>
<view class="search_content">
    <navigator class="search_item"
    wx:for="{{goods}}"
    wx:key="goods_id"
    url="/pages/goods_detail/index?goods_id={{item.goods_id}}"
    >
        {{item.goods_name}}
    </navigator>
    <view class="empty"
    wx:if="{{goods.length===0}}"
    >NothinG...</view>
</view>